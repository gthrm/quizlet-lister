{
    "manifest_version": 3,
    "name": "Quizlet Lister",
    "version": "1.0",
    "description": "Add selected words to a list for Quizlet",
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "service_worker.js"
    }
}